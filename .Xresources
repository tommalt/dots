! X colors.
! Generated by 'wal'
emacs*foreground:   #d59fd5
emacs*background:   #150c1e
URxvt*foreground:   #d59fd5
XTerm*foreground:   #d59fd5
UXTerm*foreground:  #d59fd5
URxvt*background:   [80]#150c1e
XTerm*background:   #150c1e
UXTerm*background:  #150c1e
URxvt*cursorColor:  #d59fd5
XTerm*cursorColor:  #d59fd5
UXTerm*cursorColor: #d59fd5
URxvt*borderColor:  [100]#150c1e

URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
!for pagers (less)
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

URxvt.font: xft:Inconsolata:size=9
URxvt.letterSpace: -1

URxvt.scrollBar: false
URxvt.cursorBlink: 1

! Colors 0-15.
*.color0: #150c1e
*color0:  #150c1e
*.color1: #A14E61
*color1:  #A14E61
*.color2: #A88D7C
*color2:  #A88D7C
*.color3: #513591
*color3:  #513591
*.color4: #615399
*color4:  #615399
*.color5: #484BC2
*color5:  #484BC2
*.color6: #985E9E
*color6:  #985E9E
*.color7: #d59fd5
*color7:  #d59fd5
*.color8: #956f95
*color8:  #956f95
*.color9: #A14E61
*color9:  #A14E61
*.color10: #A88D7C
*color10:  #A88D7C
*.color11: #513591
*color11:  #513591
*.color12: #615399
