! X colors.
! Generated by 'wal'
emacs*foreground:   #d59fd5
emacs*background:   #150c1e
URxvt*foreground:   #d59fd5
XTerm*foreground:   #d59fd5
UXTerm*foreground:  #d59fd5
URxvt*background:   [80]#150c1e
XTerm*background:   #150c1e
UXTerm*background:  #150c1e
URxvt*cursorColor:  magenta
XTerm*cursorColor:  #d59fd5
UXTerm*cursorColor: #d59fd5
URxvt*borderColor:  [100]#150c1e

URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
!for pagers (less)
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

URxvt.font: xft:Inconsolata:size=11
URxvt.letterSpace: -1

URxvt.scrollBar: false
URxvt.cursorBlink: 1

!perls
URxvt.perl-ext-common: default,matcher,resize-font,clipboard,url-select,keyboard-select
URxvt.colorUL: #4682B4
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-s: perl:url-select:search
!left-click to open links
URxvt.matcher.button: 1
!vim mode
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.resize-font.smaller: M-p
URxvt.resize-font.bigger:  M-n

! Colors 0-15.
*.color0: #150c1e
*color0:  #150c1e
*.color1: #A14E61
*color1:  #A14E61
*.color2: #A88D7C
*color2:  #A88D7C
*.color3: #513591
*color3:  #513591
*.color4: #615399
*color4:  #615399
*.color5: #484BC2
*color5:  #484BC2
*.color6: #985E9E
*color6:  #985E9E
*.color7: #d59fd5
*color7:  #d59fd5
*.color8: #956f95
*color8:  #956f95
*.color9: #A14E61
*color9:  #A14E61
*.color10: #A88D7C
*color10:  #A88D7C
*.color11: #513591
*color11:  #513591
*.color12: #615399
*color12:  #615399
*.color13: #484BC2
*color13:  #484BC2
*.color14: #985E9E
*color14:  #985E9E
*.color15: #d59fd5
*color15:  #d59fd5

! Black color that will not be affected by bold highlighting.
*.color66: #150c1e
*color66:  #150c1e

! Xclock colors.
XClock*foreground: #d59fd5
XClock*background: #150c1e
XClock*majorColor:  rgba:d5/9f/d5/ff
XClock*minorColor:  rgba:d5/9f/d5/ff
XClock*hourColor:   rgba:d5/9f/d5/ff
XClock*minuteColor: rgba:d5/9f/d5/ff
XClock*secondColor: rgba:d5/9f/d5/ff

! Set depth to make transparency work.
URxvt*depth: 32
