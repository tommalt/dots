! X colors.
! Generated by 'wal'
emacs*foreground:   #d59fd5
emacs*background:   #150c1e
URxvt*foreground:   #ffffff
URxvt*background:   #000000
! Set depth to make transparency work.
URxvt*depth: 32

URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
!for pagers (less)
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

URxvt.font: xft:Inconsolata:size=11
URxvt.letterSpace: -1

URxvt.scrollBar: false
URxvt.cursorBlink: 1
URxvt.pointerBlank: true
URxvt.pointerBlankDelay: 987654321

!perls
URxvt.perl-ext-common: default,matcher,resize-font,clipboard,url-select,keyboard-select,-searchable-scrollback
URxvt.colorUL: #4682B4
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.keysym.M-u: perl:url-select:select_next
!left-click to open links
URxvt.matcher.button: 1
!vim mode
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.resize-font.smaller: M-o
URxvt.resize-font.bigger:  M-p
